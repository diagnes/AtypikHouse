<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 m-b-md">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>
                {% if form.name.vars.value %}
                    Name : {{ form.name.vars.value }}
                {% else %}
                    New document index __name__
                {% endif %}
            </h5>
            <div class="ibox-tools">
                <a class="collapse-link">
                    <i class="fa fa-chevron-up"></i>
                </a>
            </div>
        </div>
        <div class="ibox-content">
            <div class="row">
                <div class="col-lg-12">
                    <div class="form-group">
                        <label class="col-sm-12">{{ form_label(form.name) }}</label>
                        <div class="col-sm-12">
                            {{ form_widget(form.name) }}
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <label class="col-sm-12">{{ form_label(form.file) }}</label>
                        <div class="col-sm-12">
                            {{ form_widget(form.file) }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
